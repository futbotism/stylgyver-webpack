{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "align": false,
    "max-line-length": [true, 130],    
    "no-switch-case-fall-through": false,
    "no-console": [
      true
    ],
    "prefer-for-of": true,
    "prefer-const": true,
    "no-unused-variable": [
      true
    ]
  }
}
